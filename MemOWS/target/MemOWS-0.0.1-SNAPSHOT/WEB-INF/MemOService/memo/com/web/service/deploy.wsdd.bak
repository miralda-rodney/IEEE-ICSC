<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from MemOService WSDL service -->

  <service name="MemO" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.web.com.memo"/>
      <parameter name="wsdlServiceElement" value="MemOService"/>
      <parameter name="schemaQualified" value="http://service.web.com.memo"/>
      <parameter name="wsdlServicePort" value="MemO"/>
      <parameter name="className" value="memo.com.web.service.MemOSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="MemO"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getProvenance" qname="operNS:getProvenance" xmlns:operNS="http://service.web.com.memo" returnQName="retNS:getProvenanceReturn" xmlns:retNS="http://service.web.com.memo" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="getOntology" qname="operNS:getOntology" xmlns:operNS="http://service.web.com.memo" returnQName="retNS:getOntologyReturn" xmlns:retNS="http://service.web.com.memo" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="getPingback" qname="operNS:getPingback" xmlns:operNS="http://service.web.com.memo" returnQName="retNS:getPingbackReturn" xmlns:retNS="http://service.web.com.memo" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getProvenance getPingback getOntology"/>

  </service>
</deployment>
